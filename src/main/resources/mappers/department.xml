<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.oa.mapper.DepartmentMapper">
  <select id="selectById" parameterType="Long" resultType="com.example.oa.entity.Department">
    select * from adm_department where department_id = #{value}
  </select>
</mapper>